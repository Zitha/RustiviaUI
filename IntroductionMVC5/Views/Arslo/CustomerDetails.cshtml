@model IntroductionMVC5.Models.ArsloTrading.ArsloCustomer

@{
    ViewBag.Title = "Arslo Customer Details";
}

<h2>Customer Details</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <label>Customer Name</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CustomerName)
        </dd>

        <dt>
            <label>Address</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Address)
        </dd>

        <dt>
            <label>Tell Number</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TellNumber)
        </dd>

    </dl>
</div>
<p>
    <input type="button" class="btn btn-primary" onclick=" location.href='@Url.Action("Index", "Arslo")'" value="Back" />
</p>
