@model IntroductionMVC5.Models.Integrator.BankAccount


@Html.ValidationSummary(true)
<fieldset>
    <div class="form-group">
        @string.Format("{0}", "Bank Name")
    </div>
    <div class="form-group">
        @Html.TextBoxFor(model => model.BankName, new { @class = "form-control", @type = "text" })
    </div>
    <div class="form-group">
        @string.Format("{0}", "Account Number")
    </div>
    <div class="form-group">
        @Html.TextBoxFor(model => model.AccountNumber, new { @class = "form-control", @type = "text" })
    </div>
    <div class="form-group">
        @string.Format("{0}", "Baranch Number")
    </div>
    <div class="form-group">
        @Html.TextBoxFor(model => model.BranchNumber, new { @class = "form-control", @type = "text" })
    </div>
    <p>
        <input type="button" class="btn btn-default" data-dismiss="modal" value="Cancel" />
        <button value="Create" class="btn btn-primary" onclick=" location.href='@Url.Action("AddBankAccount", "Supplier")' ">Save</button>
    </p>
</fieldset>

